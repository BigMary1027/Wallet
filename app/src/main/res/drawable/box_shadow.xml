    <solid android:color="#FFFFFF"/>
    <corners android:radius="10dp"/>
    <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp" />
    <stroke
        android:width="1dp"
        android:color="#25000000"
        android:dashWidth="0dp"
        android:dashGap="0dp" />
